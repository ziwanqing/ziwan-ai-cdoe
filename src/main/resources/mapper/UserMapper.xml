<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.ziwanaicodebackend.mapper.UserMapper">

    <select id="selectByAccount" resultType="com.example.ziwanaicodebackend.model.entity.User">
        SELECT * FROM user WHERE userAccount = #{userAccount} LIMIT 1
    </select>
</mapper>
